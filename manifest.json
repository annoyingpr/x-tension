  {
    "name": "X-TENSION",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
          "js/app.js"
        ]
      }
    ],
    "description": "A mental wellness Chrome extension",
    "permissions": ["storage", "notifications"],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "browser_action": {
      "default_icon": "icon.png"
    },
    "manifest_version": 2
  }